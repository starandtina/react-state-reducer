    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-eslint": "8.2.6",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^4.19.1",
